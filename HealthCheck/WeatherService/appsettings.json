{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "HealthCheck": {
    "ApiPathReadiness": "/health/ready",
    "ApiPathLiveness": "/health/live",

    "HttpUris": [ "https://www.google.com", "https://www.bing.com" ]
  }
}
